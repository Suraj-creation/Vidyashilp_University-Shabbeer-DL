{
  "name": "dl-course-platform",
  "version": "1.0.0",
  "description": "Educational Platform for Course Management with Admin Panel",
  "main": "server/index.js",
  "scripts": {
    "server": "nodemon server/index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm install && npm run build",
    "setup": "node server/setup.js",
    "start": "node server/index.js",
    "test-db": "node test-connection.js",
    "vercel-build": "cd client && npm install && npm run build"
  },
  "engines": {
    "node": "20.x"
  },
  "keywords": [
    "education",
    "course",
    "admin",
    "react",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.19.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.13",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}

{
  "name": "dl-course-platform",
  "version": "1.0.0",
  "description": "Educational Platform for Course Management with Admin Panel",
  "main": "server/index.js",
  "scripts": {
    "server": "nodemon server/index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm install && npm run build",
    "setup": "node server/setup.js",
    "start": "node server/index.js",
    "test-db": "node test-connection.js"
  },
  "keywords": ["education", "course", "admin", "react", "express"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "express-validator": "^7.0.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
